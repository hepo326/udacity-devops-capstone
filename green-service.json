{
    "kind":"Service",
    "apiVersion":"v1",
    "metadata":{
       "name":"bluegreenlb",
       "labels":{
          "app":"bluegreenlb"
       }
    },
    "spec":{
      "imagePullPolicy ": "Always",
       "ports": [
          {
            "port":80,
            "targetPort":80
          }
       ],
       "selector":{
          "app":"green"
       },
       "type": "LoadBalancer"
    },
    "strategy":{
       "type":"RollingUpdate"
    }
 }